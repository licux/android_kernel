		reg = <0x1f002000 0x0 0x1000>;
		compatible = "generic,goldfish-tty";
	};

	goldfish_pic@1f000000 {
		#interrupt-cells = <0x1>;
		phandle = <0x8000>;
		interrupt-controller;
		reg = <0x1f000000 0x0 0x1000>;
		compatible = "generic,goldfish-pic";
	};

	cpus {

		cpu@0 {
			compatible = "mti,5KEf";
			device_type = "cpu";
		};
	};

	firmware {

		android {
			revision = [31 00];
			hardware = "ranchu";
			compatible = "android,firmware";
		};
	};
};
